<% content_for :page_title do %>
  <%= link_to Spree.t(:subdistricts), spree.admin_district_subdistricts_url(@district) %> /
  <%= @subdistrict.name %>
<% end %>

<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @subdistrict } %>

<%= form_for [:admin, @district, @subdistrict] do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/admin/shared/edit_resource_links' %>
  </fieldset>
<% end %>
